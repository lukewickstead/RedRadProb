{
  "name": "redradprob",
  "version": "1.0.0",
  "description": "Prediction Calculator",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test-watch": "jest --watch --runInBand",
    "test-coverage": "jest --coverage --runInBand",
    "lint": "eslint ./ --ext .js,.jsx",
    "build-client": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "dev:build-client": "NODE_ENV=development webpack --config webpack.config.js --mode development",
    "dev:run-server": "nodemon --exitcrash --exec babel-node ./index.server.js",
    "dev": "npm run dev:build-client && npm run dev:run-server ",
    "start": "npm run dev"
  },
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "index.server.js"
    ],
    "delay": "2500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukewickstead/RedRadProb.git"
  },
  "author": "Luke Wickstead",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukewickstead/RedRadProb/issues"
  },
  "homepage": "https://github.com/lukewickstead/RedRadProb#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@types/react": "^15.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.3.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.1",
    "sass": "^1.3.0",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "core-js": "^3.4.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.1",
    "fibers": "^4.0.2",
    "history": "^4.10.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.4",
    "react-dom": "^16.12.0",
    "react-google-charts": "^3.0.15",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.3",
    "win-node-env": "^0.4.0"
  }
}
